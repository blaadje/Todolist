{
  "name": "todolist",
  "version": "0.29.5",
  "author": {
    "name": "blaadje",
    "email": "acharlot91@gmail.com"
  },
  "description": "Todo list application to be more efficient",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently -r -p \"{name}\" -n \"ðŸš‹ Webpack,ðŸ”Œ Electron\" -c \"green.bold,yellow.bold\" \"webpack-dev-server --config=./config/dev/webpack.config.js --mode development\" \"sleep 2 && electron ./src/main.js\"",
    "foo": "webpack-dev-server --color --progress --config=./config/dev/webpack.config.js --mode development",
    "build": "node config/prod/build.js",
    "dist": "electron-builder",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "stylelint": "stylelint 'src/components/**/style.module.scss' --fix"
  },
  "build": {
    "productName": "Todolist",
    "appId": "io.blaadje.todo-list",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons"
    }
  },
  "dependencies": {
    "@ckpack/vue-color": "^0.0.3",
    "@sentry/browser": "^5.18.1",
    "@vuex-orm/core": "^0.36.4",
    "localforage": "^1.9.0",
    "universal-analytics": "^0.4.20",
    "vue": "^3.1.1",
    "vue-color": "^2.3.2",
    "vue3-click-outside": "andymark-by/vue3-click-outside",
    "vuex": "^4.0.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "eslint-plugin-prettier": "^3.4.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.10.1",
    "@vue/babel-plugin-jsx": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^8.0.6",
    "prettier": "^2.2.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "concurrently": "^5.0.2",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.2.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.2.0",
    "electron-updater": "^4.3.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^7.10.0",
    "html-webpack-plugin": "^4.3.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.13.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.18.1",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^3.0.1",
    "vue-svg-loader": "^0.17.0-beta.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
